<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.dampevillage.domain">

	<class name="RoomCategory" table="room_category">

		<id name="id" column="room_category_id" type="int">
			<generator class="increment" />
		</id>

		<property name="roomCategoryType" type="string" column="room_categoty_type"/>
		
		<property name="description" type="string" column="description"/>
		
		<property name="imageName" type="string" column="image_name"/>
		
		
	</class>
</hibernate-mapping>
